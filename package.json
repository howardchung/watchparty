    "@types/react-dom": "^17.0.0",
    "@types/react-onclickoutside": "^6.7.3",
    "@types/react-router-dom": "^5.1.5",
    "@types/recharts": "^1.8.16",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.35",
    "@types/uuid": "^7.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "2.0.5",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.3"
  }
