{
  "name": "string2compact",
  "description": "Convert 'hostname:port' strings to BitTorrent's compact ip/host binary returned by Trackers",
  "version": "2.0.1",
  "author": {
    "name": "WebTorrent, LLC",
    "email": "feross@webtorrent.io",
    "url": "https://webtorrent.io"
  },
  "bugs": {
    "url": "https://github.com/webtorrent/string2compact/issues"
  },
  "type": "module",
  "dependencies": {
    "addr-to-ip-port": "^2.0.0",
    "ipaddr.js": "^2.0.0"
  },
  "devDependencies": {
    "@webtorrent/semantic-release-config": "1.0.8",
    "compact2string": "1.4.1",
    "semantic-release": "19.0.5",
    "standard": "*",
    "tape": "5.6.3"
