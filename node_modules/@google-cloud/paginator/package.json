    "@types/node": "^16.0.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.0",
    "@types/uuid": "^8.0.0",
    "c8": "^7.0.0",
    "codecov": "^3.0.4",
    "gts": "^3.0.0",
    "linkinator": "^2.0.0",
    "mocha": "^8.0.0",
    "proxyquire": "^2.0.1",
    "sinon": "^13.0.0",
    "typescript": "^3.8.3",
