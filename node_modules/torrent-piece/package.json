    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/feross"
    },
    {
      "type": "consulting",
      "url": "https://feross.org/support"
    }
  ],
  "renovate": {
    "extends": [
      "github>webtorrent/renovate-config"
    ]
  },
  "dependencies": {
    "uint8-util": "^2.1.9"
  }
}
