    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "airtap": "^4.0.4",
    "airtap-manual": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-minify": "^0.5.2",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "prettier-bytes": "^1.0.4",
    "speedometer": "^1.1.0",
    "standard": "^17.1.0",
    "tape": "^5.8.1"
  },
  "homepage": "https://github.com/thaunknown/simple-websocket",
  "keywords": [
    "simple",
    "simple websocket",
